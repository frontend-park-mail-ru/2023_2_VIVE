{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": [
    "public/js/templates/*.js",
    "dist/*"
  ],

  "rules": {
    "no-unused-vars": "off"
  }
}
