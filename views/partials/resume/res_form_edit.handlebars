<form class="js-edit-form" data-page={{page}}>
    {{> (concat 'res_form_' page) (object errors=errors data=data page_data=page_data)}}
    <div class="form__actions">

        <button type="submit" class="js-save-edit link-btn link-btn-blue--bg text-light-title"
            data-page={{page}}>Сохранить</button>

        {{> link-btn
        text="Отменить изменения"
        data=(object page=page)
        class="js-cancel-edit link-btn-blue text-dodger-blue"}}
    </div>
</form>
