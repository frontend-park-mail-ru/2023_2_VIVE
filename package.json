{
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "nodemon": "^3.0.1",
    "path": "^0.12.7"
  },
  "scripts": {
    "start": "nodemon --exec \"npm run compile:templates && npm run compile:partials && node server/server.js\"",
    "compile:templates": "handlebars views/layouts/*.handlebars views/pages/*.handlebars -f views/templates.precompiled.js",
    "compile:partials": "handlebars views/partials/*.handlebars -p -f views/partials.precompiled.js",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "prettier": "3.0.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
